  "scripts": {
    "ng": "ng",
    "start": "concurrently -r \"npm run start:server\" \"npm run start:client\" ",
    "start:server": "NODE_ENV=development DEBUG=ZumaiaTour* nodemon server/index.js --exec babel-node",
    "start:client": "ng server",
    "start-prod": "NODE_ENV=production node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm run build:client && npm run build:server",
    "build:client": "ng build --environment=prod",
    "build:server": "babel server --out-dir dist/server",
    "postinstall": "npm run build"
  },
